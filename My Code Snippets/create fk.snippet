<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
<_locDefinition xmlns="urn:locstudio">
    <_locDefault _loc="locNone" />
    <_locTag _loc="locData">Title</_locTag>
    <_locTag _loc="locData">Description</_locTag>
    <_locTag _loc="locData">Author</_locTag>
    <_locTag _loc="locData">ToolTip</_locTag>
</_locDefinition>
	<CodeSnippet Format="1.0.0">
	<Header>
	<Title>Create fk</Title>
        <Shortcut></Shortcut>
	<Description>Creates fk</Description>
	<Author>Quan</Author>
	<SnippetTypes>
		<SnippetType>Expansion</SnippetType>
	</SnippetTypes>
	</Header>
	<Snippet>
		<Declarations>
			<Literal>
				<ID>ct_name</ID>
				<ToolTip>Name of the constraint</ToolTip>
				<Default>fk_table1_table2</Default>
			</Literal>
			<Literal>
				<ID>fk_name</ID>
				<ToolTip>Name of the foreign key</ToolTip>
				<Default>fkname</Default>
			</Literal>
			<Literal>
				<ID>pk_name</ID>
				<ToolTip>Name of the primary key</ToolTip>
				<Default>pkname</Default>
			</Literal>
		</Declarations>
		<Code Language="SQL">
			<![CDATA[constraint $ct_name$ 
	foreign key $fk_name$ references $pk_name$,
$end$]]>
		</Code>
	</Snippet>
	</CodeSnippet>
</CodeSnippets>

